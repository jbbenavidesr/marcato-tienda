    "dependencies": {
        "@11ty/eleventy": "^0.12.1",
        "@11ty/eleventy-cache-assets": "^2.3.0",
        "@11ty/eleventy-img": "^0.10.0",
        "contentful": "^8.4.2",
        "dotenv": "^10.0.0",
        "npm-run-all": "^4.1.5",
        "rollup": "^2.56.2",
        "rollup-plugin-terser": "^7.0.2",
        "sass": "^1.35.1"
    },
    "browserslist": [
        "last 2 versions"
